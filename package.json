{
  "name": "vitest-failed-summary-reporter",
  "version": "0.0.2",
  "description": "A vitest reporter that prints a summary of failed tests",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "module": "build/index.js",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "keywords": [
    "vitest",
    "vitest reporter",
    "test fail summary"
  ],
  "author": "https://github.com/hjoelh",
  "license": "MIT",
  "files": [
    "build"
  ],
  "scripts": {
